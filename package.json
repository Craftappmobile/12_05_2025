{
  "name": "expo-supabase-starter",
  "version": "1.0.0",
  "main": "./index.js",
  "scripts": {
    "start": "npx expo start",
    "start:clear": "npx expo start --clear",
    "start:port": "npx expo start --port 8083",
    "start:reset": "npx expo start --clear --port 8083",
    "web": "npx expo start --web",
    "android": "npx expo run:android",
    "android:dev": "npx expo start --clear && npx expo run:android",
    "ios": "npx expo run:ios",
    "lint": "eslint . --fix",
    "clean": "rm -rf node_modules && rm -rf .expo && rm -rf dist && npm install --legacy-peer-deps",
    "clean:windows": "rmdir /s /q node_modules && rmdir /s /q .expo && rmdir /s /q dist && npm install --legacy-peer-deps",
    "reset-metro": "npx react-native start --reset-cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:conflictresolver": "jest '__tests__/modules/sync/ConflictResolver.test.ts'",
    "test:migrationmanager": "jest '__tests__/modules/migrations/MigrationManager.test.ts'"
  },
  "dependencies": {

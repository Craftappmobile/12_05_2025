{
  "threshold": 5,
  "reporters": ["html", "console"],
  "ignore": [
    "**/node_modules/**",
    "**/.git/**",
    "**/build/**",
    "**/dist/**",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx"
  ],
  "format": [
    "typescript",
    "tsx",
    "javascript",
    "jsx"
  ],
  "output": "./jscpd-report",
  "minLines": 5,
  "maxLines": 1000
}